body
    font Helvetica, Arial, "Lucida Sans"
    color #eee
    background white

#container
    width 80%
    height 90%
    min-height 1024px
    margin-left auto
    margin-right auto
    background #434
    margin-top 2em
    padding 1em
    border-radius 15px

a
    color #66f

#buttons a
    padding 4px
    border-radius 5px
    color red
    background white
    margin 1em
    width 124px
    text-decoration none

#buttons
    background #602
    padding-top 1em
    padding-bottom 1em
    border-radius 3px
    text-align center

#header
    text-align center

#footer
    padding-top 100px
    font-size small
    color #966
